{
  "name": "commit-genius",
  "version": "1.5.0",
  "description": "AI-powered commit message generator using Google Gemini",
  "main": "dist/index.js",
  "bin": {
    "genius": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "commit": "npm run build && node dist/index.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "git",
    "commit",
    "ai",
    "gemini",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "typescript": "^5.5.0",
    "ts-node": "^10.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
